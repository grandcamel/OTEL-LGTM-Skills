{
  "dashboard": {
    "id": null,
    "uid": "claude-code-overview",
    "title": "Claude Code - Overview",
    "tags": ["claude-code", "overview"],
    "timezone": "browser",
    "schemaVersion": 39,
    "version": 1,
    "refresh": "30s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Total Cost (USD)",
        "type": "stat",
        "gridPos": { "h": 4, "w": 6, "x": 0, "y": 0 },
        "datasource": { "type": "loki", "uid": "loki" },
        "targets": [{
          "expr": "sum by() (sum_over_time({service_name=\"claude-code\", event_name=\"api_request\"} | json | unwrap cost_usd [$__range]))",
          "refId": "A"
        }],
        "options": {
          "colorMode": "value",
          "graphMode": "none",
          "justifyMode": "auto",
          "textMode": "auto",
          "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }
        },
        "fieldConfig": {
          "defaults": {
            "unit": "currencyUSD",
            "decimals": 4,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "color": "green", "value": null },
                { "color": "yellow", "value": 1 },
                { "color": "red", "value": 10 }
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "title": "Total Input Tokens",
        "type": "stat",
        "gridPos": { "h": 4, "w": 6, "x": 6, "y": 0 },
        "datasource": { "type": "loki", "uid": "loki" },
        "targets": [{
          "expr": "sum by() (sum_over_time({service_name=\"claude-code\", event_name=\"api_request\"} | json | unwrap input_tokens [$__range]))",
          "refId": "A"
        }],
        "options": {
          "colorMode": "value",
          "graphMode": "none",
          "justifyMode": "auto",
          "textMode": "auto",
          "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "mode": "absolute",
              "steps": [{ "color": "blue", "value": null }]
            }
          }
        }
      },
      {
        "id": 3,
        "title": "Total Output Tokens",
        "type": "stat",
        "gridPos": { "h": 4, "w": 6, "x": 12, "y": 0 },
        "datasource": { "type": "loki", "uid": "loki" },
        "targets": [{
          "expr": "sum by() (sum_over_time({service_name=\"claude-code\", event_name=\"api_request\"} | json | unwrap output_tokens [$__range]))",
          "refId": "A"
        }],
        "options": {
          "colorMode": "value",
          "graphMode": "none",
          "justifyMode": "auto",
          "textMode": "auto",
          "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "mode": "absolute",
              "steps": [{ "color": "purple", "value": null }]
            }
          }
        }
      },
      {
        "id": 4,
        "title": "API Requests",
        "type": "stat",
        "gridPos": { "h": 4, "w": 6, "x": 18, "y": 0 },
        "datasource": { "type": "loki", "uid": "loki" },
        "targets": [{
          "expr": "count_over_time({service_name=\"claude-code\", event_name=\"api_request\"} [$__range])",
          "refId": "A"
        }],
        "options": {
          "colorMode": "value",
          "graphMode": "none",
          "justifyMode": "auto",
          "textMode": "auto",
          "reduceOptions": { "calcs": ["sum"], "fields": "", "values": false }
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "mode": "absolute",
              "steps": [{ "color": "green", "value": null }]
            }
          }
        }
      },
      {
        "id": 5,
        "title": "Cost Over Time",
        "type": "timeseries",
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 4 },
        "datasource": { "type": "loki", "uid": "loki" },
        "targets": [{
          "expr": "sum by(model) (sum_over_time({service_name=\"claude-code\", event_name=\"api_request\"} | json | unwrap cost_usd [1m]))",
          "legendFormat": "{{model}}",
          "refId": "A"
        }],
        "options": {
          "legend": { "displayMode": "list", "placement": "bottom" },
          "tooltip": { "mode": "multi" }
        },
        "fieldConfig": {
          "defaults": {
            "unit": "currencyUSD",
            "custom": {
              "drawStyle": "bars",
              "fillOpacity": 80,
              "stacking": { "mode": "normal" }
            }
          }
        }
      },
      {
        "id": 6,
        "title": "Tokens Over Time",
        "type": "timeseries",
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 4 },
        "datasource": { "type": "loki", "uid": "loki" },
        "targets": [
          {
            "expr": "sum_over_time({service_name=\"claude-code\", event_name=\"api_request\"} | json | unwrap input_tokens [1m])",
            "legendFormat": "Input",
            "refId": "A"
          },
          {
            "expr": "sum_over_time({service_name=\"claude-code\", event_name=\"api_request\"} | json | unwrap output_tokens [1m])",
            "legendFormat": "Output",
            "refId": "B"
          }
        ],
        "options": {
          "legend": { "displayMode": "list", "placement": "bottom" },
          "tooltip": { "mode": "multi" }
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "custom": {
              "drawStyle": "bars",
              "fillOpacity": 80,
              "stacking": { "mode": "normal" }
            }
          }
        }
      },
      {
        "id": 7,
        "title": "Cost by Model",
        "type": "piechart",
        "gridPos": { "h": 8, "w": 8, "x": 0, "y": 12 },
        "datasource": { "type": "loki", "uid": "loki" },
        "targets": [{
          "expr": "sum by(model) (sum_over_time({service_name=\"claude-code\", event_name=\"api_request\"} | json | unwrap cost_usd [$__range]))",
          "legendFormat": "{{model}}",
          "refId": "A"
        }],
        "options": {
          "legend": { "displayMode": "table", "placement": "right", "values": ["value", "percent"] },
          "pieType": "pie",
          "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }
        },
        "fieldConfig": {
          "defaults": { "unit": "currencyUSD" }
        }
      },
      {
        "id": 8,
        "title": "Cache Efficiency",
        "type": "piechart",
        "gridPos": { "h": 8, "w": 8, "x": 8, "y": 12 },
        "datasource": { "type": "loki", "uid": "loki" },
        "targets": [
          {
            "expr": "sum(sum_over_time({service_name=\"claude-code\", event_name=\"api_request\"} | json | unwrap cache_read_tokens [$__range]))",
            "legendFormat": "Cache Read",
            "refId": "A"
          },
          {
            "expr": "sum(sum_over_time({service_name=\"claude-code\", event_name=\"api_request\"} | json | unwrap cache_creation_tokens [$__range]))",
            "legendFormat": "Cache Creation",
            "refId": "B"
          },
          {
            "expr": "sum(sum_over_time({service_name=\"claude-code\", event_name=\"api_request\"} | json | unwrap input_tokens [$__range]))",
            "legendFormat": "New Input",
            "refId": "C"
          }
        ],
        "options": {
          "legend": { "displayMode": "table", "placement": "right", "values": ["value", "percent"] },
          "pieType": "donut",
          "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }
        },
        "fieldConfig": {
          "defaults": { "unit": "short" }
        }
      },
      {
        "id": 9,
        "title": "API Latency (p50/p95/p99)",
        "type": "timeseries",
        "gridPos": { "h": 8, "w": 8, "x": 16, "y": 12 },
        "datasource": { "type": "loki", "uid": "loki" },
        "targets": [
          {
            "expr": "quantile_over_time(0.50, {service_name=\"claude-code\", event_name=\"api_request\"} | json | unwrap duration_ms [5m]) by ()",
            "legendFormat": "p50",
            "refId": "A"
          },
          {
            "expr": "quantile_over_time(0.95, {service_name=\"claude-code\", event_name=\"api_request\"} | json | unwrap duration_ms [5m]) by ()",
            "legendFormat": "p95",
            "refId": "B"
          },
          {
            "expr": "quantile_over_time(0.99, {service_name=\"claude-code\", event_name=\"api_request\"} | json | unwrap duration_ms [5m]) by ()",
            "legendFormat": "p99",
            "refId": "C"
          }
        ],
        "options": {
          "legend": { "displayMode": "list", "placement": "bottom" },
          "tooltip": { "mode": "multi" }
        },
        "fieldConfig": {
          "defaults": {
            "unit": "ms",
            "custom": { "drawStyle": "line", "fillOpacity": 10 }
          }
        }
      },
      {
        "id": 10,
        "title": "Recent API Requests",
        "type": "table",
        "gridPos": { "h": 8, "w": 24, "x": 0, "y": 20 },
        "datasource": { "type": "loki", "uid": "loki" },
        "targets": [{
          "expr": "{service_name=\"claude-code\", event_name=\"api_request\"} | json",
          "refId": "A"
        }],
        "transformations": [
          {
            "id": "extractFields",
            "options": { "source": "labels" }
          },
          {
            "id": "organize",
            "options": {
              "includeByName": {
                "Time": true,
                "model": true,
                "input_tokens": true,
                "output_tokens": true,
                "cache_read_tokens": true,
                "cost_usd": true,
                "duration_ms": true
              }
            }
          }
        ],
        "options": {
          "showHeader": true,
          "sortBy": [{ "displayName": "Time", "desc": true }]
        },
        "fieldConfig": {
          "defaults": {},
          "overrides": [
            { "matcher": { "id": "byName", "options": "cost_usd" }, "properties": [{ "id": "unit", "value": "currencyUSD" }] },
            { "matcher": { "id": "byName", "options": "duration_ms" }, "properties": [{ "id": "unit", "value": "ms" }] }
          ]
        }
      }
    ]
  },
  "overwrite": true
}
